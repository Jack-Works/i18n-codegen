{
  "name": "@magic-works/i18n-codegen",
  "packageManager": "pnpm@7.14.2",
  "version": "0.4.1",
  "type": "module",
  "description": "A code generator for strongly-typed i18n resources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -p src -w",
    "build": "tsc -p src",
    "release": "pnpm run build && pnpm test && npx changeset publish",
    "test": "vitest"
  },
  "bin": "./dist/cli.js",
  "keywords": [
    "i18n",
    "typescript"
  ],
  "author": "Jack Works",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@types/node": "^18.11.9",
    "@types/yargs": "^17.0.13",
    "jest-file-snapshot": "^0.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "i18next-translation-parser": "^1.0.1",
    "yargs": "^17.6.0"
  },
  "peerDependencies": {
    "typescript": "^4.8 || ^5"
  },
  "files": [
    "dist",
    "src",
    "schema.json"
  ]
